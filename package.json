    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swagger-ui-react": "^3.35.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.7",
    "@types/react": "^16.9.51",
    "@types/swagger-ui-react": "^3.23.3",
    "swagger-jsdoc": "^4.2.3",
    "typescript": "^4.0.3"
  }
}
